{
    "name": "divide-framework",
    "version-string": "0.1",
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "dependencies": [
        "boost-serialization",
        "boost-property-tree",
        "boost-asio",
        "stb",
        "freetype",
        "fmt",
        "concurrentqueue",
        "devil",
        "eastl",
        "simpleini",
        "glbinding",
        "glm",
        "meshoptimizer",
        "nvtt",
        "sdl2",
        "sdl2-image",
        {
            "name": "sdl2-mixer",
            "features": [
                "fluidsynth",
                "libflac",
                "libmodplug",
                "mpg123",
                "opusfile"
            ]
        },
        "openal-soft",
        "physx",
        "expat",
        "pkgconf",
        "recastnavigation",
        "assimp",
        {
            "name": "imgui",
            "features": [ "docking-experimental", "no-obsolete-keyio", "no-obsolete-functions" ]
        },
        "imgui-node-editor",
        "imguizmo",
        "zlib",
        "vulkan",
        "vulkan-memory-allocator",
        "glslang",
        "ctre",
        "catch2"
    ],
    "builtin-baseline": "2ac6ba173be85207a7bac4f0f02f5486f9fd4c0f"
}