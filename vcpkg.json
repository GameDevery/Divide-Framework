{
    "name": "divide-framework",
    "version-string": "0.1",
    "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
    "dependencies": [
        "pkgconf",
        "boost-property-tree",
        "boost-asio",
        "stb",
        "freetype",
        "fmt",
        "concurrentqueue",
        "devil",
        "eastl",
        "simpleini",
        "glbinding",
        "glm",
        "meshoptimizer",
        "nvtt",
        "openal-soft",
        "physx",
        "expat",
        "recastnavigation",
        "imgui-node-editor",
        "imguizmo",
        "vulkan",
        "vulkan-memory-allocator",
        "vk-bootstrap",
        "glslang",
        "ctre",
        "catch2",
        "fribidi",
        "assimp",
        "sdl2",
        "sdl2-image",
        {
            "name": "sdl2-mixer",
            "features": [
                "fluidsynth",
                "libflac",
                "libmodplug",
                "mpg123",
                "wavpack",
                "opusfile"
            ]
        },
        {
            "name": "imgui",
            "features": [ "docking-experimental", "no-obsolete-keyio", "no-obsolete-functions" ]
        },
        {
            "name": "mimalloc",
            "features": []
        }
    ]
}