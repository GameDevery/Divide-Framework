<!-- Add a new terrain here by settings it's name in the terrainList --><terrainList>	<name>test</name>	<!--<name>terrain</name>-->	<!--<name>terrain2</name>--></terrainList>		<!-- For each terrain listed in the terrainList tag, add a new node named like the terrain with the terrain's details --><!-- @Param: heightmap - represents the file to be used for generating the terrain's geometry --><!-- @Param: scale - represents the width and lenght factor to be multplied by the heightmap's resolution --><!-- @Param: heightFactor - represents the height factor of the terrain (each y-components of each triangle's nodes is multiplied by this--> <!-- @Param: position - add the x/y/z position in global space as attributes for this tag --><!-- @Param: active - set to true if you want the terrain to be loaded. False to skip --><!-- @Param: vegetation - set the vegetation displacement map using this tag. Use it's attribute's to tweak vegetation information --><!-- @Param: textures - enclosed in this node are the various texture settings --><!-- @Param: textures.map - the texture displacement map. Each channel in this texture corresponds to the placement of a different texture --><!-- @Params: red/green/blue/alpha - textures to be placed as specified in each channel of the displacement map --><test>	<heightmap>teren/test.jpg</heightmap>	<scale>1</scale>	<heightFactor>1</heightFactor>	<position x="0" y="0" z="0" />	<active>true</active>	<vegetation grassDensity="3333" treeDensity="0" grassScale="0.3" treeScale="0.7">		<map>misc_images/test_heightmap_diffusemap.jpg</map>		<grassBillboard1>misc_images/grass_billboards1.tga</grassBillboard1>		<grassBillboard2>misc_images/grass_billboards2.tga</grassBillboard2>		<grassBillboard3>misc_images/grass_billboards3.tga</grassBillboard3>	</vegetation>	<textures>		<map>misc_images/test_heightmap_diffusemap.jpg</map>		<normalMap>misc_images/terrain_detail_NM.png</normalMap>		<red>misc_images/sandfloor009a.jpg</red>		<green>misc_images/terrain_rocky_map_1024.png</green>		<blue>misc_images/terrain_grass_map_1024.png</blue>		<waterCaustics>misc_images/terrain_water_caustics.jpg</waterCaustics>	</textures></test><terrain>	<heightmap>teren\terrain.tga</heightmap>	<scale>1</scale>	<heightFactor>30</heightFactor>	<position x="-30" y="-40" z="0" />	<active>false</active>	<vegetation grassDensity="200" treeDensity="20" grassScale="1" treeScale="1">		<map>misc_images/test_heightmap512_2_diffusemap.jpg</map>		<grassBillboard1>misc_images/grass_billboards1.tga</grassBillboard1>		<grassBillboard2>misc_images/grass_billboards2.tga</grassBillboard2>		<grassBillboard3>misc_images/grass_billboards3.tga</grassBillboard3>	</vegetation>	<textures>		<map>misc_images/test_heightmap512_2_diffusemap.jpg</map>		<normalMap>misc_images/terrain_detail_NM.tga</normalMap>		<red>misc_images/sandfloor009a.jpg</red>		<green>misc_images/terrain_rocky_map_1024.png</green>		<blue>misc_images/terrain_grass_map_1024.png</blue>		<waterCaustics>misc_images/terrain_water_caustics.jpg</waterCaustics>	</textures></terrain><terrain2>	<heightmap>teren\terrain2.tga</heightmap>	<scale>20</scale>	<heightFactor>50</heightFactor>	<position x="0" y="100" z="400" />	<active>false</active>	<vegetation grassDensity="200" treeDensity="20" grassScale="1" treeScale="1">		<map>misc_images/test_heightmap512_2_diffusemap.jpg</map>		<grassBillboard1>misc_images/grass_billboards1.tga</grassBillboard1>		<grassBillboard2>misc_images/grass_billboards2.tga</grassBillboard2>		<grassBillboard3>misc_images/grass_billboards3.tga</grassBillboard3>	</vegetation>	<textures>		<map>misc_images/test_heightmap512_2_diffusemap.jpg</map>		<normalMap>misc_images/terrain_detail_NM.tga</normalMap>		<red>misc_images/sandfloor009a.jpg</red>		<green>misc_images/terrain_rocky_map_1024.png</green>		<blue>misc_images/terrain_grass_map_1024.png</blue>		<waterCaustics>misc_images/terrain_water_caustics.jpg</waterCaustics>	</textures></terrain2>